# Example Environment Variables Configuration
# Copy this file to .env and update with your actual values

# Google Cloud Project Configuration
PROJECT_ID=your-gcp-project-id
CLUSTER_NAME=your-cluster-name
ARTIFACT_REGISTRY_REPO_NAME=your-repo

# Domain Configuration
ASSISTANT_DOMAIN=assistant.yourdomain.com
ONLINE_BOUTIQUE_DOMAIN=ob.yourdomain.com
BASE_DOMAIN=yourdomain.com

# Google AI API Key for Gemini Integration
GOOGLE_AI_API_KEY=your-gemini-api-key

# Optional: Region Configuration
REGION=your-region


# Optional: Terraform Backend Configuration
TERRAFORM_BACKEND_BUCKET=your-terraform-state-bucket
TERRAFORM_BACKEND_PREFIX=your-terraform-state-prefix/terraform/state
